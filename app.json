{
  "expo": {
    "name": "Lollipop",
    "slug": "lollipop-mobile",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/appIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shubh997.lollipopmobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shubh997.lollipopmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3ca269c4-eb63-4130-85fc-90dcc7403c93"
      }
    },
    "runtimeVersion": "1.0.6",
    "updates": {
      "url": "https://u.expo.dev/3ca269c4-eb63-4130-85fc-90dcc7403c93"
    }
  }
}
